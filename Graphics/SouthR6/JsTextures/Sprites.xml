<?xml version="1.0" encoding="utf-8" ?>
<Sprites>

  <theo_crystal path="SouthR6/JsTextures/characters/theoCrystal/" start="idle">
    <Origin x="32" y="42"/>
    <Loop id="idle" path="idle" delay="0.08"/>
    <Anim id="shatter" path="shatter" delay="0.08" goto="shattered"/>
    <Loop id="shattered" path="shatter" frames="16" delay="0.08"/>
  </theo_crystal>

  <swapBlockLight path="SouthR6/JsTextures/objects/swapblock/" start="idle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="idle" path="midBlock" delay="0.08" frames="0-3"/>
  </swapBlockLight>
  <swapBlockLightRed path="SouthR6/JsTextures/objects/swapblock/" start="idle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="idle" path="midBlockRed" delay="0.08" frames="0-3"/>
  </swapBlockLightRed>

  <swapBlockPathLight path="SouthR6/JsTextures/objects/swapblock/" start="idle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="idle" path="pathLights" delay="0.06" frames="0"/>
    <Anim id="flash" path="pathLights" delay="0.06" frames="0-4" goto="idle"/>
  </swapBlockPathLight>

  <dashSwitch_default path="SouthR6/JsTextures/objects/temple/dashButton" start="idle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="idle" path="" delay="0.08" frames="0-20"/>
    <Loop id="pushed" path="" delay="0.08" frames="27"/>
    <Anim id="push" path="" delay="0.07" frames="21-27" goto="pushed"/>
  </dashSwitch_default>

  <dashSwitch_mirror path="SouthR5/JsTextures/objects/temple/dashButtonMirror" start="idle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="idle" path="" delay="0.08" frames="0-20"/>
    <Loop id="pushed" path="" delay="0.08" frames="27"/>
    <Anim id="push" path="" delay="0.07" frames="21-27" goto="pushed"/>
  </dashSwitch_mirror>

  <booster path="SouthR6/JsTextures/objects/booster/" start="loop">
    <Justify x="0.5" y="0.5"/>
    <Loop id="loop" path="booster" delay="0.1" frames="0-4"/>
    <Loop id="inside" path="booster" delay="0.1" frames="5-8"/>
    <Loop id="spin" path="booster" delay="0.06" frames="18-25"/>
    <Anim id="pop" path="booster" delay="0.08" frames="9-17"/>
  </booster>
  <boosterRed path="SouthR6/JsTextures/objects/booster/" start="loop">
    <Justify x="0.5" y="0.5"/>
    <Anim id="appear" path="boosterRed" delay="0.08" frames="26-34" goto="loop"/>
    <Loop id="loop" path="boosterRed" delay="0.1" frames="0-4"/>
    <Loop id="inside" path="boosterRed" delay="0.1" frames="5-8"/>
    <Loop id="spin" path="boosterRed" delay="0.06" frames="18-25"/>
    <Anim id="pop" path="boosterRed" delay="0.08" frames="9-17"/>
  </boosterRed>

  <heartgem0 path="SouthR6/JsTextures/collectables/heartGem/0/">
    <Center />

    <Loop id="idle" path="" frames="0" />
    <Loop id="spin" path="" frames="0*10,1-13" delay="0.1"/>
    <Loop id="fastspin" path="" delay="0.1"/>
  </heartgem0>

  <key path="SouthR6/JsTextures/collectables/key/" start="idle">
    <Justify x=".5" y=".5" />
    <Loop id="idle" path="idle" delay="0.1" frames="0-13"/>
    <Anim id="enter" path="enter" delay="0.1"/>
  </key>

  <bumpBlockCenterIce path="SouthR6/JsTextures/objects/BumpBlockNew/" start="idle">
    <Center/>
    <Loop id="idle" path="ice_center" delay="0.1"/>
  </bumpBlockCenterIce>

  <bumpBlockCenterFire path="SouthR6/JsTextures/objects/BumpBlockNew/" start="idle">
    <Center/>
    <Loop id="idle" path="fire_center" delay="0.08"/>
  </bumpBlockCenterFire>

  <glider path="SouthR6/JsTextures/objects/glider/" start="idle">
    <Justify x="0.5" y="0.58"/>
    <Loop id="idle" path="idle" delay="0.1"/>
    <Loop id="held" path="held" delay="0.1"/>
    <Anim id="fall" path="fall" delay="0.06" goto="fallLoop"/>
    <Loop id="fallLoop" path="fallLoop" delay="0.06"/>
    <Anim id="death" path="death" delay="0.06"/>
  </glider>

  <refill path="SouthR6/JsTextures/objects/refill/" start="idle">
    <Justify x="0.5" y="0.5"/>
    <Loop id="idle" path="idle" delay="0.1"/>
    <Anim id="flash" path="flash" delay="0.05"/>
    <Loop id="oneuse_idle" path="idle" delay="0.1"/>

    <!-- you could adds this, 
	     unlike oneuse_idle, it works only when if player using another mod "BetterRefillGems" -->
    <!-- <Loop id="idlenr" path="idlenr" delay="0.1"/>  -->
  </refill>

  <refillTwo path="SouthR6/JsTextures/objects/refillTwo/" copy="refill"/>

</Sprites>
